    "convert-migrations": "./db/bin/convert-migrations.sh",
    "lint": "turbo lint"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "turbo": "^2.1.3"
  },
  "prisma": {
    "schema": "./db/prisma/schema/schema.prisma"
  },
