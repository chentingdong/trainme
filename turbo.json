{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "build-app": {
      "dependsOn": ["db:generate", "^build"],
      "outputs": ["dist/**"],
      "env": []
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "start": {
      "dependsOn": ["^build", "build"]
    },
    "dev": {
      "dependsOn": ["^db:generate"],
      "persistent": true,
      "cache": false,
      "inputs": ["$TURBO_DEFAULT$", ".env.local"]
    },
    "db:generate": {
      "cache": false
    },
    "lint:eslint": {},
    "lint:prettier": {}
  }
}
